{
   "manifest_version": 2,
   "name": "Exchange Rate Table",
   "short_name": "FX Rate Table",
   "version": "1.0",
   "description": "Display an exchange rate table.",
   "browser_action": {
      "default_icon": "icons/128.png",
      "default_popup": "popup.html"
   },
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "48": "icons/48.png"
   },
   "permissions": [ "http://*/*" ]
}